{
  "name": "libtest",
  "version": "0.1.0",
  "main": "dist/index.js",
  "umd:main": "dist/libtest.umd.production.js",
  "module": "dist/libtest.es.production.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "scripts": {
    "postinstall": "tsdx build",
    "lint": "eslint '**/*.ts'",
    "lint:fix": "eslint '**/*.ts' --fix",
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "pretty-quick && eslint '**/*.ts' --fix && tsdx test"
  },
  "standard": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint/eslint-plugin"
    ]
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "@typescript-eslint/eslint-plugin": "^1.10.2",
    "@typescript-eslint/parser": "^1.10.2",
    "eslint": "^5.16.0",
    "husky": "^2.4.1",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "tsdx": "^0.6.1",
    "tslib": "^1.10.0",
    "typescript": "^3.5.2"
  },
  "dependencies": {
    "lodash": "^4.17.11"
  }
}
